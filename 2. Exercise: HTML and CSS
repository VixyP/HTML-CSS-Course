<!-- 5. Simple Navigation Bar -->

<!-- simple-nav-bar.html -->


<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Simple Navigation Bar</title>
    <link rel="stylesheet" href="simple-nav-bar.css">
</head>
<body>
    <nav>
        <ul>
            <li><a href="#">Home</a></li>
            <li><a href="#">Services</a></li>
            <li><a href="#">Products</a></li>
            <li><a href="#">About Us</a></li>
            <li><a href="#">Contact</a></li>
        </ul>
    </nav>
</body>
</html>


